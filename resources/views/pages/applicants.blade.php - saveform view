						var result = $(this).attr('id');//.split('-');
						if(result){
						var name         = result;//[0];/* //(split - after name ) */
						}
						row[name] = 0;
					}
				} 
				else {
					
					row[$(this).attr('name')] = $(this).val();
				}
			});
			//row['item_order'] = item_order;
			//row['step_id'] = step_id;
			item_order++;
			
			items.push(row);
		});
		
